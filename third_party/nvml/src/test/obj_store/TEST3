export UNITTEST_NAME=obj_store/TEST3
export UNITTEST_NUM=3

# standard unit test setup
. ../unittest/unittest.sh

setup

create_nonzeroed_file 8 4 $DIR/testfile1

expect_normal_exit ./obj_store$EXESUFFIX $DIR/testfile1 u

check

pass
