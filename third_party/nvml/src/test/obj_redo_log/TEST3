#
# src/test/obj_redo_log/TEST3 -- unit test for redo_log_process function
#
export UNITTEST_NAME=obj_redo_log/TEST3
export UNITTEST_NUM=3

# standard unit test setup
. ../unittest/unittest.sh

setup

FILE=${DIR}/pool
FSIZE=$((1024*1024))
RSIZE=4

truncate -s $FSIZE $FILE

expect_normal_exit ./obj_redo_log$EXESUFFIX $FILE $RSIZE\
	C\
	f:0:0x00002008:0x01010101\
	r:0x00002008\
	P\
	r:0x00002008\
	e:0\
	s:0:0x00002010:0x11111111\
	s:1:0x00002018:0x22222222\
	s:2:0x00002020:0x33333333\
	f:3:0x00002028:0x44444444\
	r:0x00002010\
	r:0x00002018\
	r:0x00002020\
	r:0x00002028\
	P\
	r:0x00002010\
	r:0x00002018\
	r:0x00002020\
	r:0x00002028\
	e:0\
	e:1\
	e:2\
	e:3\
	C\
	s:0:0x00002100:0xaaaaaaaa\
	s:1:0x00002108:0xbbbbbbbb\
	s:2:0x00002110:0xcccccccc\
	f:3:0x00002118:0xdddddddd\
	r:0x00002100\
	r:0x00002108\
	r:0x00002110\
	r:0x00002118\
	P\
	r:0x00002100\
	r:0x00002108\
	r:0x00002110\
	r:0x00002118\
	e:0\
	e:1\
	e:2\
	e:3\
	C

check

pass
