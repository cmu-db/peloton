#
# src/test/obj_list_macro/TEST6 -- unit test for list API
#
export UNITTEST_NAME=obj_list_macro/TEST6
export UNITTEST_NUM=6

# standard unit test setup
. ../unittest/unittest.sh

setup

expect_normal_exit ../obj_list_macro/obj_list_macro$EXESUFFIX $DIR/testfile\
	n:0:0:1 P:1 P:2\
	n:0:0:2 P:1 P:2\
	n:0:0:3 P:1 P:2\
	n:0:0:4 P:1 P:2\
	n:0:0:5 P:1 P:2\
	n:0:0:6 P:1 P:2\
	m:3:0:0 P:1 P:2\
	m:2:0:0 P:1 P:2\
	r:1:1 P:1 P:2\
	m:0:1:1 P:1 P:2\
	n:0:0:7 P:1 P:2\
	n:0:0:8 P:1 P:2\
	n:0:0:9 P:1 P:2\
	n:0:0:10 P:1 P:2\
	n:0:0:11 P:1 P:2\
	f:1:6 P:1 P:2\
	f:1:5 P:1 P:2\
	m:0:-1:1 P:1 P:2\
	m:0:1:-4 P:1 P:2\
	m:2:-1:3 P:1 P:2\
	r:2:2 P:1 P:2\
	m:0:1:0 P:1 P:2\
	f:2:2 P:1 P:2\
	m:0:0:1 P:1 P:2

check

pass
